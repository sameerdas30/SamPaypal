<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mycompany.cordova.myshop" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="CardIO">
        <param name="android-package" value="io.card.cordova.sdk.CardIOCordovaPlugin" />
    </feature>
    <feature name="PayPalMobile">
        <param name="android-package" value="com.paypal.cordova.sdk.PayPalMobileCordovaPlugin" />
    </feature>
    <name>MyCordovaShop</name>
    <description>
        A sample Apache Cordova application that responds to the deviceready event.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
